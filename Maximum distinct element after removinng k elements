for i in range(int(input())):
    a,b=map(int,input().split())
    l=[int(i) for i in input().split()]
    l1=[]
    for i in l:
        if i not in l1:
            l1.append(i)
        else:
            b-=1
    if b<=0:
        print(len(l1))
    else:
        print(len(l1)-b)
    
            
    
            
